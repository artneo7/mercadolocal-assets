			"button {",
			"  color: var(--principal);",
			"  border: none;",
			"  cursor: pointer;",
			"  padding: 0;",
			"  background: none;",
			"  text-underline-offset: 2px;",
			"  touch-action: manipulation;",
			"}",
			":focus-visible {",
			"  outline: 1px auto var(--principal);",
			"  outline-offset: 2px;",
			"}",
			"input[disabled] {",
			"  cursor: not-allowed;",
			"  background-color: var(--bg);",
			"  color: var(--principal-2);",
			"}"
